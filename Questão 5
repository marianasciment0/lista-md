num1 = int(input("informe o primeiro numero: "))
num2 = int(input("informe o segundo n√∫mero: "))
anterior = num1
atual = num2
resultado = anterior%atual

while resultado != 0:
    anterior = atual
    atual = resultado
    resultado = anterior%atual

print("mdc({},{})= {})".format(num1,num2, atual))
